<script>
  var shaneProswiper = new Swiper('#{{ section.id }}-product-swiper', {
    loop: true,
    speed: 200,
    navigation: {
      nextEl: '#{{ section.id }}-product-swiper .shaneProSwiper-prev',
      prevEl: '#{{ section.id }}-product-swiper .shaneProSwiper-next',
    },
    pagination: {
      el: '#{{ section.id }}-product-swiper .swiper-pagination',
    },
    on: {
      init: function () {
        swiperAnimateCache(this); //隐藏动画元素
        swiperAnimate(this); //初始化完成开始动画
      },
      slideChangeTransitionEnd: function () {
        swiperAnimate(this); //每个slide切换结束时也运行当前slide动画
        //this.slides.eq(this.activeIndex).find('.ani').removeClass('ani'); 动画只展现一次，去除ani类名
      },
    },
  });
  $(document).ready(function () {
    $('#{{ section.id }}-product-swiper .more').click(function () {
      $(this).css('display', 'none');
      $('#{{ section.id }}-product-swiper .info').css('height', 'auto');
    });
  });
</script>
