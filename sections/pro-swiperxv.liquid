</style>
	<div class="swiper shaneProSwiperxv">
		<div class="context">
			<h2>{{ section.settings.heading }}</h2>
			<div>{{ section.settings.text }}</div>
		</div>
		<div class="swiper-wrapper">
            {%- for block in section.blocks -%}
			<div class="swiper-slide">
				<img src="{{ block.settings.block_image_bg | image_url: width: 1920,height:1006 }}"  class="slide-bg pc_show">
				<img src="{{ block.settings.block_image_bg_m | image_url }}" class="slide-bg mobile_show">
				<img src="{{ block.settings.block_image | image_url: width: 735,height:852 }}"  class="slide-img1 ani" swiper-animate-effect="fadeInLeft" swiper-animate-duration="0.5s">
				<div class="slide-context {{ block.settings.color }}">
					<div>
						<div class="slide-type" style="background-color:{{ block.settings.top_button_color }}"><img src="{{ block.settings.block_image_icon | image_url }}"><div>{{ block.settings.block_subtitle }}</div></div>
						<div class="slide-title">{{ block.settings.block_title }}</div>
						{% if block.settings.border_button_color != blank %}
							{% assign border_button_color = block.settings.border_button_color %}
						{% else %} 
							 {% assign border_button_color = block.settings.color %}
						 {% endif %}
                        {% if block.settings.text_disguise != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">Disguise</div><div>{{ block.settings.text_disguise }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.text_height != blank %}
						<div class="slide-cs" style="border-color: {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">Height</div><div>{{ block.settings.text_height }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.text_strengths != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">Strengths</div><div>{{ block.settings.text_strengths }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.text_weaknesses != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">Weaknesses</div><div>{{ block.settings.text_weaknesses }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.text_position != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">Position</div><div>{{ block.settings.text_position }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.text_features != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">Features</div><div>{{ block.settings.text_features }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.custom_title_1 != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">{{ block.settings.custom_title_1 }}:</div><div>{{ block.settings.custom_text_1 }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.custom_title_2 != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">{{ block.settings.custom_title_2 }}:</div><div>{{ block.settings.custom_text_2 }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.custom_title_3 != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">{{ block.settings.custom_title_3 }}:</div><div>{{ block.settings.custom_text_3 }}</div>
						</div>
                        {% endif %}
                        {% if block.settings.custom_title_4 != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">{{ block.settings.custom_title_4 }}:</div><div>{{ block.settings.custom_text_4 }}</div>
						</div>
                        {% endif %}
					
                        {% if block.settings.custom_title_5 != blank %}
						<div class="slide-cs" style="border-color:  {{ border_button_color }}">
							<div style="background-color:{{ border_button_color }}">{{ block.settings.custom_title_5 }}:</div><div>{{ block.settings.custom_text_5 }}</div>
						</div>
                        {% endif %}
						<div class="slide-flex" style="border-color: {{ border_button_color }}"> 
							{% if block.settings.custom_title_6 != blank %}
								<div class="slide-flex-title" style="background-color:  {{ border_button_color }}">{{ block.settings.custom_title_6 }}</div>
							
							<div class="slide-grid">
								{% if block.settings.custom_image_1 != blank %}
									<div class="slide-grid-item">
										<img src="{{ block.settings.custom_image_1 | image_url }}" class="slide-grid-item-img">
										<div>{{ block.settings.custom_image_text_1 }}</div>
									</div>
								{% endif %}
								{% if block.settings.custom_image_2 != blank %}
									<div class="slide-grid-item">
										<img src="{{ block.settings.custom_image_2 | image_url }}" class="slide-grid-item-img">
										<div>{{ block.settings.custom_image_text_2 }}</div>
									</div>
								{% endif %}
								{% if block.settings.custom_image_3 != blank %}
									<div class="slide-grid-item">
										<img src="{{ block.settings.custom_image_3 | image_url }}" class="slide-grid-item-img">
										<div>{{ block.settings.custom_image_text_3 }}</div>
									</div>
								{% endif %}
								{% if block.settings.custom_image_4 != blank %}
									<div class="slide-grid-item">
										<img src="{{ block.settings.custom_image_4 | image_url }}" class="slide-grid-item-img">
										<div>{{ block.settings.custom_image_text_4 }}</div>
									</div>
								{% endif %}
								{% if block.settings.custom_image_5 != blank %}
									<div class="slide-grid-item">
										<img src="{{ block.settings.custom_image_5 | image_url }}" class="slide-grid-item-img">
										<div>{{ block.settings.custom_image_text_5 }}</div>
									</div>
								{% endif %}
							</div>
						{% endif %}
						</div>

                        
					</div>
					<div class="swiper-ps"><span></span> {{ block.settings.block_ps }}</div>
				</div>
			</div>
            {% endfor %}
		</div>


		<div class="shaneProSwiperxv-next">
			<img src="https://cdn.shopify.com/s/files/1/0679/6538/6990/files/icon-right.svg?v=1705634939" class="img-np">
			<img src="https://cdn.shopify.com/s/files/1/0679/6538/6990/files/icon-left.svg?v=1705634939" class="img-np-hover">
		</div>
		<div class="shaneProSwiperxv-prev">
			<img src="https://cdn.shopify.com/s/files/1/0679/6538/6990/files/icon-right.svg?v=1705634939" class="img-np">
			<img src="https://cdn.shopify.com/s/files/1/0679/6538/6990/files/icon-left.svg?v=1705634939" class="img-np-hover">
		</div>
	</div>
