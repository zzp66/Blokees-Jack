<style>
	.shane-pro-swiperxv{
		background-color: #1A1A1A; padding-top: 6vw; color: #fff;
	}
	.shane-pro-swiperxv>div{
		position: relative; width: 100%;
	}
	.shaneProSwiperxv .context{
		width: 46%; position: absolute; top: 0; left: 27%; text-align: center; z-index: 99;        width: calc(100vw* 1062 / 1920);
        left: calc(100vw* 453 / 1920);
		display: flex;
        width: calc(100vw* 1262 / 1920);
        left: calc(100vw* 324 / 1920);
        flex-direction: column;
        justify-content: center;
        align-items: center;
	}
	.shaneProSwiperxv .context h2{
		margin: 0; font-size: 2.6vw; color:#fff; line-height:3.2vw;        width: calc(100vw* 1062 / 1920);
	}
	.shaneProSwiperxv .context div{
		width: 98%;  font-size: 1vw; opacity: 0.8;width: calc(100vw* 1062 / 1920);
	}
	.shaneProSwiperxv .swiper-slide{
		position: relative;
	}
	.shaneProSwiperxv .swiper-slide .slide-bg{
		width:100%;
	}
	.shaneProSwiperxv .swiper-slide .slide-img1{
		width:38.3vw; position: absolute; bottom: 0; left: 3vw;
	}
	.shaneProSwiperxv .slide-context{
		position: absolute; width:50vw; bottom: 2.4vw; left: 45vw; height: 35vw; display: flex; flex-wrap: wrap; align-content: space-between;
	}
	.slide-context>div{
		width:100%;
	}
	.shaneProSwiperxv .slide-type{
		width: 15vw; display:flex; justify-content: flex-start; align-items: center; border-radius: 8px;
	}
	.shaneProSwiperxv .slide-type img{
		width: 2.9vw;
	}
	.shaneProSwiperxv .slide-type div{
		font-size:1.4vw; text-transform: uppercase; padding: 0 1vw; font-family: Outfit-Bold;
	}
	.shaneProSwiperxv .slide-title{
		 font-family: Outfit-Bold; font-size: 5vw; text-transform: uppercase;
	}
	.shaneProSwiperxv .slide-cs{
		border: 1px solid #D60023;
		display:flex; justify-content: flex-start; align-items: center; width: fit-content; height: 1.88vw; border-radius: 0.94vw; line-height: 1.7vw; font-size: 1vw; margin-bottom: 0.5vw; text-transform: capitalize;
	}
	.shaneProSwiperxv .slide-cs>div:first-child{
		width: 7.8vw; height: 100%; text-align: center; border-radius: 0.94vw;
	}
	.shaneProSwiperxv .slide-cs>div:last-child{
		height: 100%; padding: 0 1.5vw 0 1vw;
	}
	.shaneProSwiperxv .slide-flex{
		border-radius: 0.94vw;
		border: 1px solid #D60023;
	}
	.shaneProSwiperxv .slide-flex .slide-flex-title{
		background-color: #D60023;
		height: 100%;
		text-align: center;
		padding:calc(100vw* 5 / 1920) calc(100vw* 21 / 1920);
		width: fit-content;
		border-radius: 0.94vw;
	}
	.shaneProSwiperxv .slide-flex .slide-grid{
		width: 100%;
		{% comment %} max-width: calc(100vw* 751 / 1920); {% endcomment %}
		display: grid;
		grid-template-columns: repeat(5, 1fr);
		margin-top: calc(100vw* 10 / 1920);
		/* border: 1px; */
		border-radius: calc(100vw* 18 / 1920);
		{% comment %} border: calc(100vw* 2 / 1920) solid #D60023; {% endcomment %}
		padding:  calc(100vw* 17 / 1920) calc(100vw* 37 / 1920);
	}
	.shaneProSwiperxv .slide-flex .slide-grid .slide-grid-item{
		display: flex;
		flex-direction: column;
		justify-content: center;
		align-items: center;
	}
	.shaneProSwiperxv .slide-flex .slide-grid .slide-grid-item div{
		max-width:calc(100vw* 130 / 1920);
		color: #FFF;
		font-family: Outfit;
		font-size: calc(100vw* 20 / 1920);
		font-style: normal;
		font-weight: 400;
		text-align: center;
		line-height: normal;
		margin-top: calc(100vw* 16 / 1920);
	}
	.shaneProSwiperxv .swiper-ps{
		font-size: 0.9vw;
	}
	.shaneProSwiperxv .swiper-ps span{
		font-size:1vw; font-family: Outfit-Bold;
	}
	.shaneProSwiperxv .purple .slide-type{
		background-color: #561C7F;
	}
	.shaneProSwiperxv .purple .slide-cs{
		border: 1px solid #561C7F;
	}
	.shaneProSwiperxv .purple .slide-cs>div:first-child{
		background-color: #561C7F;
	}
	.shaneProSwiperxv .red .slide-type{
		background-color: #D60023;
	}
	.shaneProSwiperxv .red .slide-cs{
		border: 1px solid #D60023;
	}
	.shaneProSwiperxv .red .slide-cs>div:first-child{
		background-color: #D60023;
	}
	.shaneProSwiperxv-next, .shaneProSwiperxv-prev{
		width: 3vw; height: 3vw; border-radius: 50%; background-color: #fff; position: absolute; top: 55%; z-index: 9; display: flex; justify-content: center; align-items: center;
	}
	.shaneProSwiperxv-next img, .shaneProSwiperxv-prev img{
		width: 40%;
	}
	.shaneProSwiperxv-next{
		left: 3vw;
	}	
	.shaneProSwiperxv-next .img-np{
		transform: scaleX(-1);
	}
	.shaneProSwiperxv-prev{
		right:3vw;
	}
	.shaneProSwiperxv-prev .img-np-hover{
		transform: scaleX(-1);
	}
	.shaneProSwiperxv .img-np-hover{
		display: none;
	}
	.shaneProSwiperxv-next:hover, .shaneProSwiperxv-prev:hover{
		background-color: #FF5F00;
	}
	.shaneProSwiperxv-next:hover .img-np, .shaneProSwiperxv-prev:hover .img-np{
		display: none;
	}
	.shaneProSwiperxv-next:hover .img-np-hover, .shaneProSwiperxv-prev:hover .img-np-hover{
		display: block;
	}
@media (max-width: 1300px){

    .shaneProSwiperxv .context{
		{% comment %} width: 56%; left: 22%; {% endcomment %}
		width: calc(100vw* 1262 / 1920); 
        left: calc(100vw* 324 / 1920);
