<style>
	.shane-pro-block06{
		background-color: #1A1A1A; padding-top: 6vw;
	}
	.shane-pro-block06>div{
		width: 90%; margin: auto; color: #fff;
	}
	.shane-pro-block06 h2{
		text-align: center; width: 65%; margin: 0 auto 1vw; font-size: 2.2vw; line-height: 2.8vw;
	}
	.shane-pro-block06 .stitle{
		text-align: center; width: 65%; margin: 0 auto 0.6vw; font-size: 1.4vw;
	}
	.shane-pro-block06 .info{
		width:70%; margin: auto; font-size: 0.9vw;opacity: 0.8; text-align: center; overflow: hidden; height: 5vw; position: relative;
	}
	.shane-pro-block06 .more{
		width:100%; position: absolute; bottom: 0; left: 0; height: 3vw; text-align: center; background-image: linear-gradient(#00000000, #1A1A1A, #1A1A1A); font-family: Outfit-Bold;
	}
	.shane-pro-block06 a{
		color:#fff; line-height: 3vw; display: flex; justify-content: center; align-items: center;
	}
	.shane-pro-block06 .more span{
		color: #FF5F00; font-size: 1.4vw;
	}
	.shane-pro-block06 .columns{
		display: flex; justify-content: start; margin-top: 2vw; flex-wrap: wrap;
	}
	.shane-pro-block06 .columns>div{
		border:1px solid #fff; border-radius: 0.6vw; overflow: hidden; margin-bottom: 0.8vw;
	}
	.shane-pro-block06 .equal-two{
		width: 23.5%; margin-right:12px;
	}
	.shane-pro-block06  .columns-img{
		width: 100%; height: 21vw; position: relative;
	}
	.shane-pro-block06  .columns-img img{
		width:100%; position: absolute; top: 0; left: 0; transition:0.2s;
	}
	.shane-pro-block06 .equal-two:hover .columns-img img{
		animation: moveUpAndDown01 1s ease-in-out infinite;
	}
	@keyframes moveUpAndDown01 {
	  0% {
		top: 0;
	  }
	  50% {
		top: -10px;
	  }
	  100% {
		top: 0;
	  }
	}
	.shane-pro-block06 .columns-context{
		width: 76%; margin: 0 auto 1vw;
	}	
	.shane-pro-block06 .columns-context .title{
		display: flex; justify-content: flex-start; align-items: center; font-size: 1.2vw; font-family: Outfit-Bold;
	}
	.shane-pro-block06 .columns-context img{
		width:1.6vw; margin-right: 0.5vw;
	}
	.shane-pro-block06 .equal-two .probability{
		width:100%; border:1px solid #fff; height: 2vw; background-color: #fff; margin-top: 5px; border-radius: 3px; display: flex; justify-content: space-between; text-align: center; font-family: Outfit-Bold;
	}
	.equal-two .probability>div:first-child{
		width: 54%; font-size: 0.8vw; color: #FF5F00; line-height: 0.8vw; display: flex; justify-content: center; align-items: center; flex-wrap: wrap;
	}
	.equal-two .probability>div:first-child span{
		font-size: 0.7vw;
	}
	.equal-two .probability>div:last-child{
		width: 46%; font-size: 1.4vw; color: #fff; background-color: #FF5F00; display: flex; justify-content: center; align-items: center; border-radius: 3px;
	}
	.shane-pro-block06 .equal-six{
		width: 49.5%; position: relative; display: flex; justify-content: flex-start; align-items: center;
	}	
	.shane-pro-block06 .equal-six>img{
		width: 60%;
	}
	.equal-six>div{
		position: absolute; top: 16%; right: 5%; width: 17vw; text-align: center;
	}
	.equal-six .title{
		font-size: 2.2vw; text-transform: uppercase; font-family: Outfit-Bold;
	}
	.equal-six .probability{
		width:95%; margin: 0.6vw auto 1.5vw;
	}
	.equal-six .probability>div:first-child{
		width: 100%; font-size: 0.8vw;
	}
	.equal-six .probability>div:last-child{
		width: 100%; line-height: 5.6vw; font-size: 5vw; font-family: Outfit-Bold; background-color: #FF5F00; border-radius: 1vw; margin-top: 0.4vw;
	}
	.equal-six .text{
		font-size: 0.75vw; width:95%; margin: auto;
	}
@media (max-width: 1300px){
    .shane-pro-block06 .info{
      font-size:1.1vw;
      height:auto;
    }
}
@media (max-width: 912px){
    .shane-pro-block06 .equal-two {
      width: 23.2%;
      margin-right: 10px;
  }
}
@media (max-width: 768px){
	.shane-pro-block06{
		padding-top: 10vw;
	}
	.shane-pro-block06 h2{
		width: 100%; margin: 0 auto 4vw; font-size: 5vw; line-height: 6.4vw;
	}
	.shane-pro-block06 .stitle{
		width: 100%; margin: 0 auto 2vw; font-size: 4vw;
	}
	.shane-pro-block06 .info{
		width:100%; font-size: 3.6vw; height: 25vw;
	}
	.shane-pro-block06 .more{
		height: 10vw;
	}
	.shane-pro-block06 a{
		line-height: 10vw;
	}
	.shane-pro-block06 .more span{
		font-size: 3.6vw;
	}
	.shane-pro-block06 .columns{
		margin-top: 6vw; justify-content: space-between;
	}
	.shane-pro-block06 .columns>div{
		border-radius: 1vw; margin-bottom: 2vw;
	}
	.shane-pro-block06 .equal-two{
		width: 48%; margin-right: 0;
	}
	.shane-pro-block06  .columns-img{
		height: 47vw;
	}
	.shane-pro-block06 .columns-context{
		width: 96%; margin: 0 auto 1vw; left: 2%;
	}	
	.shane-pro-block06 .columns-context .title{
		font-size: 3vw;
	}
	.shane-pro-block06 .columns-context img{
		width:3.4vw; margin-right: 1vw;
	}
	.shane-pro-block06 .equal-two .probability{
		height: 8vw;
	}
	.equal-two .probability>div:first-child{
		font-size: 2.4vw;line-height: 0vw;
	}
	.equal-two .probability>div:first-child span{
		font-size: 2.2vw;
	}
	.equal-two .probability>div:last-child{
		font-size: 5vw;
	}
	.shane-pro-block03 .equal-six{
		width: 100%;
	}	
	.shane-pro-block03 .equal-six>img{
		width: 60%;
	}
	.equal-six>div{
		width: 36vw;top: 5%; right: 4%;
	}
	.equal-six .title{
		font-size: 4.6vw;
	}
	.equal-six .probability{
		margin: 0 auto 2vw;
	}
	.equal-six .probability>div:first-child{
		width: 100%; font-size: 3.2vw;
	}
	.equal-six .probability>div:last-child{
		line-height: 9vw; font-size: 6vw; margin-top: 1vw;
	}
	.equal-six .text{
		font-size: 2.4vw; width:100%; margin: auto;
	}
}
</style>
	<div>
		<h2>{{ section.settings.heading }}</h2>
		<div class="stitle">{{ section.settings.subheadingheading }}</div>
		<div class="info">
			{{ section.settings.text }}
		</div>
		<div class="columns">
          {%- assign my_variable = 0 -%} 
          {%- for block in section.blocks -%}
            {% assign my_variable = my_variable | plus: 1 %}
            {% if my_variable>4 %}
            {% assign delay = my_variable | minus: 4 %}
            {% else %}
            {% assign delay = my_variable | minus: 1 %}
            {% endif %}
            {% if block.settings.stylewidth == '1' %}
			<div class="equal-two wow fadeInUp" data-wow-duration=".5s" data-wow-delay=".{{ delay }}s" data-wow-offset="100">
				<div class="columns-img">
					<img src="{{ block.settings.block_image | image_url }}" loading="lazy">
					{% comment %} <picture>
						<source
						  media="(max-width: 800px)"
						  srcset="{{ block.settings.block_image | image_url: width: 208,height:254 }}"
						/>
						<img src="{{ block.settings.block_image | image_url: width: 298,height:363 }}" loading="lazy"/>
					  </picture> {% endcomment %}
				</div>	
				<div class="columns-context">
					<div class="title">{% if block.settings.block_image_icon != blank %}<img src="{{ block.settings.block_image_icon | image_url }}">{% endif %} {{ block.settings.block_title }}</div>
                    {% if block.settings.block_text != blank %}
					<div class="probability">
						<div>Probability<br><span>(Math.)</span></div>
						<div>{{ block.settings.block_text }}</div>
					</div>
                    {% endif %}  
				</div>
			</div>
            {% else %}
			<div class="equal-six wow fadeInUp" data-wow-duration=".5s" data-wow-delay=".5s" data-wow-offset="100">
				<img src="{{ block.settings.block_image | image_url }}">
				<div>
					<div class="title">{{ block.settings.block_title }}</div>
					<div class="probability">
						<div>Probability(Math.)</div>
						<div>{{ block.settings.block_text }}</div>
					</div>
					<div class="text">{{ block.settings.block_text_02 }}</div>
				</div>
			</div>
            {% endif %}            
          {% endfor %}
		</div>	
	</div>
{% schema %}
  {
    "name": "Pro block06",
    "class": "shane-pro-block06",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading"
	  },
      {
        "type": "text",
        "id": "subheading",
        "label": "Subheading"
	  },
      {
        "id": "text",
        "type": "textarea",
        "label": "Text"
      }   
    ],
    "blocks": [
       {
         "type": "item",
         "name": "item",
         "settings": [
           {
              "type": "select",
              "id": "stylewidth",
              "options": [
                {
                  "value": "1",
                  "label": "small"
                },
                {
                  "value": "2",
                  "label": "Big"
                }
              ],
              "default": "1",
              "label": "Width"
            },
           {
              "type": "image_picker",
              "id": "block_image",
              "label": "Image"
            },
           {
              "type": "image_picker",
              "id": "block_image_icon",
              "label": "Image icon",
              "info": "icon"
            },
           {
              "type": "text",
              "id": "block_title",
              "label": "Title",
              "default": "Add a title"
			},
           {
              "type": "text",
              "id": "block_text",
              "label": "Probability"
			},
           {
              "type": "text",
              "id": "block_text_02",
              "label": "Text"
			}
         ]
       }
    ],
    "presets": [
       {
        "name": "Pro block06"
          
       }
    ]
  }
{% endschema %}
