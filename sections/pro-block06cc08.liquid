          "info": "icon"
        },
        {
          "type": "text",
          "id": "block_title",
          "label": "Title",
          "default": "Add a title"
        },

        {
          "type": "text",
          "id": "block_text",
          "label": "Probability"
        },
        {
          "type": "text",
          "id": "block_text_02",
          "label": "Text"
        },
        {
          "id": "slide_link",
          "type": "url",
          "label": "Slide link",
          "info": "Adds a link to slide"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Pro block06cc08"
    }
  ]
}
{% endschema %}
