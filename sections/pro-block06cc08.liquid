<div>
  <div class="block06cc08-fflex">
    <h2>{{ section.settings.heading }}</h2>
    {% comment %} <div class="stitle">{{ section.settings.subheadingheading }}</div> {% endcomment %}
    <div class="info">
      {{ section.settings.text }}
    </div>
  </div>
  <div class="columns">
    {%- assign my_variable = 0 -%}
    {%- for block in section.blocks -%}
      {% assign my_variable = my_variable | plus: 1 %}
      {% if my_variable > 4 %}
        {% assign my_variable = 1 %}
      {% endif %}
      {% comment %}
        {% if my_variable>4 %}
        {% assign \ = my_variable | minus: 4 %}
        {% else %}
        {% assign delay = my_variable | minus: 1 %}
        {% endif %}
      {% endcomment %}
      {% if block.settings.stylewidth == '1' %}
        <div
          class="equal-two wow fadeInUp"
          data-num="{{ my_variable }}"
          data-wow-duration=".5s"
          data-wow-delay=".{{ my_variable }}s"
          data-wow-offset="100"
        >
          <div class="columns-img">
            <picture>
              <source
                media="(max-width: 800px)"
                srcset="{{ block.settings.block_image | image_url: width: 560 }}"
              >
              <img src="{{ block.settings.block_image | image_url: width: 724 }}" loading="lazy">
            </picture>
            {% comment %} <img src="{{ block.settings.block_image | image_url }}" loading="lazy"> {% endcomment %}
          </div>
          <div class="columns-context">
            <div class="title">
              <div class="growdC">
                {% if block.settings.block_image_icon != blank %}
                  <img class="block06cc08_ico" src="{{ block.settings.block_image_icon | image_url }}">
                  <img class="block06cc08_hover_ico" src="{{ block.settings.block_image_hover_icon | image_url }}">
                {% endif %}
                <div class="growdC-p">
                  {{ block.settings.block_title }}
                </div>
              </div>
              <a href="{{ block.settings.slide_link  }}" class="btn">
                <div class="btnTxst">Shop Now</div>
                <img class="btnSvg" src="{{ '5456.svg' | asset_url }}">
                {% comment %}
                  <svg class='btnSvg' xmlns="http://www.w3.org/2000/svg" width="12" height="11" viewBox="0 0 12 11" fill="none">
