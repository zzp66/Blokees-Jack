	.b3-3-steps{
		width: 100%; text-align: center; display: flex; justify-content: center; align-items: center; position: relative; z-index: 2;
	}
	.b3-3-steps>div{
		width:27vw; height: 27vw; border:1px solid #fff; border-radius: 50%;
	}
	.b3-3-steps .stitle{
		margin-top: 8vw; font-size: 2.1vw; line-height: 2.5vw; font-family: Outfit-Bold;
	}
	.b3-3-steps .stext{
		width: 70%; margin: 1vw auto 0; font-size: 1.5vw; line-height: 2.2vw;
	}
	.b3-img1{
