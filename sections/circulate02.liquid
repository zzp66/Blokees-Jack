<style>
	.shane-circulate{
		width:100%; height: 90px; padding: 36px 0 16px; background-position: top; background-repeat: repeat-x; overflow: hidden; position: relative;
	}
	.circulate-style02{
		 background-image: url(https://cdn.shopify.com/s/files/1/0679/6538/6990/files/bg-blue.png?v=1706096278); 
	}
	.circulate-style02 span{
		color: #006DE5; font-family: Outfit-Bold; font-size: 20px;
	}
	.circulate-content{
		width: 100%;
		margin: 0;
		padding: 0;		
		display: flex;  
		justify-content:space-around;
		align-items: center;
		position: absolute;  
		will-change: transform;  
		animation: shaneScroll 15s linear infinite;
	}
	.circulate-content-clone {  
		position: absolute;  
		left: 100%;  
	  } 
	@keyframes shaneScroll {
		 0% { transform: translateX(0); }  
    	100% { transform: translateX(-100%); }
	}
	.circulate-content img{
		height: 100%;
	}
@media (max-width: 1200px){
	.circulate-content{
		width: 130%;
	}
	.circulate-content-clone {  
		position: absolute;  
		left: 130%;  
	}
}
@media (max-width: 990px){
	.circulate-content{
		width: 160%;
	}
	.circulate-content-clone {  
		left: 160%;  
	}
}
@media (max-width: 768px){
	.circulate-content{
		width: 230%;
	}
	.circulate-content-clone {  
		left: 230%;  
	}
}
@media (max-width: 556px){
	.circulate-content{
		width: 300%;
	}
	.circulate-content-clone {  
		left: 300%;  
	}
}
</style>
