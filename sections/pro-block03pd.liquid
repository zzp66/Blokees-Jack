        },
        {
          "type": "text",
          "id": "block_big_text1",
          "label": "Big * Content1"
        },
        {
          "type": "text",
          "id": "block_big_text2",
          "label": "Big * Content2"
        },
        {
          "type": "text",
          "id": "block_big_text3",
          "label": "Big * Content3"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Pro block03pd"
    }
  ]
}
