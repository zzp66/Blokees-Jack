<div>
  <h2>{{ section.settings.heading }}</h2>
  <div class="stitle">{{ section.settings.subheadingheading }}</div>
  <div class="info">
    {{ section.settings.text }}
    {% if section.settings.yc_checkbox == false %}
      <div class="more">
        <a>Read More&nbsp;&nbsp;<span class="fa fa-arrow-circle-down"></span></a>
      </div>
    {% endif %}
  </div>
  <div class="columns">
    {%- assign my_variable = 0 -%}
    {%- for block in section.blocks -%}
      {% assign my_variable = my_variable | plus: 1 %}
      {% if my_variable > 6 %}
        {% assign delay = my_variable | minus: 6 %}
      {% else %}
        {% assign delay = my_variable | minus: 1 %}
      {% endif %}
      {% if block.settings.stylewidth == '1' %}
        <div
          class="equal-two wow fadeInUp"
          data-wow-duration=".5s"
          data-wow-delay=".{{ delay }}s"
          data-wow-offset="100"
        >
          <div class="columns-img">
            {% comment %} <img src="{{ block.settings.block_image | image_url }}" loading="lazy"> {% endcomment %}
            <picture>
              <source
                media="(max-width: 800px)"
                srcset="{{ block.settings.block_image | image_url: width: 208 }}"
              >
              <img src="{{ block.settings.block_image | image_url: width: 540 }}" loading="lazy">
            </picture>
          </div>
          <div class="columns-context">
            <div class="title">
              {% if block.settings.block_image_icon != blank %}
                <img src="{{ block.settings.block_image_icon | image_url }}">
              {% endif %}
              {{ block.settings.block_title }}
            </div>
            <div class="probability">
              <div>
                Probability<br>
                <span>(Math.)</span>
              </div>
              <div>{{ block.settings.block_text }}</div>
            </div>
          </div>
        </div>
      {% endif %}
      {% if block.settings.stylewidth == '2' %}
        <div class="equal-six wow fadeInUp" data-wow-duration=".5s" data-wow-delay=".5s" data-wow-offset="100">
          <img src="{{ block.settings.block_image | image_url }}">
          <div>
            <div class="top-title">{{ block.settings.block_top_title }}</div>
            <div class="title">{{ block.settings.block_title }}</div>
            <div class="probability">
              <div>Probability (Math.)</div>
              <div>{{ block.settings.block_text }}</div>
            </div>
            <div class="text">{{ block.settings.block_text_02 }}</div>
          </div>
        </div>
      {% endif %}
      {% if block.settings.stylewidth == '3' %}
        <div
          class="equal-six equal-six2 wow fadeInUp"
          data-wow-duration=".5s"
          data-wow-delay=".5s"
          data-wow-offset="100"
        >
          <img class="pc_show" src="{{ block.settings.block_image | image_url }}">
          <img class="mobile_show" src="{{ block.settings.block_mb_image | image_url }}">
          <div class="left">
            <div class="title">
              <div class="mtitle">{{ block.settings.block_title }}</div>
              <div class="text">{{ block.settings.block_text_02 }}</div>
            </div>
            <div class="grop">
              <div class="item">
                {{ block.settings.block_big_text1 }}
              </div>
              <div class="item">
                {{ block.settings.block_big_text2 }}
              </div>
              <div class="item">
                {{ block.settings.block_big_text3 }}
              </div>
            </div>
          </div>
          {% comment %}
            <div class='right'>
                <div class='top-title'>{{ block.settings.block_top_title }}</div>
                <div class="title">{{ block.settings.block_title }}</div>
                <div class="probability">
                    <div>Probability (Math.)</div>
                    <div>{{ block.settings.block_text }}</div>
                </div>
                <div class="text">{{ block.settings.block_text_02 }}</div>
            </div>
          {% endcomment %}
        </div>
      {% endif %}
      {% if block.settings.stylewidth == '4' %}
        <div
          class="equal-six equal-six3 wow fadeInUp"
          data-wow-duration=".5s"
          data-wow-delay=".5s"
          data-wow-offset="100"
        >
          <img class="pc_show" src="{{ block.settings.block_image | image_url }}">
          <img class="mobile_show" src="{{ block.settings.block_mb_image | image_url }}">
          <div class="left">
            <div class="title">
              {% if block.settings.block_image_icon != blank %}
                <img src="{{ block.settings.block_image_icon | image_url }}">
              {% endif %}
              {{ block.settings.block_new_text }}
            </div>
            <div class="grop">
              <div class="item">
                {{ block.settings.block_big_text1 }}
              </div>
              <div class="item">
                {{ block.settings.block_big_text2 }}
              </div>
              <div class="item">
                {{ block.settings.block_big_text3 }}
              </div>
            </div>
          </div>
          <div class="right">
            <div class="top-title">{{ block.settings.block_top_title }}</div>
            <div class="title">{{ block.settings.block_title }}</div>
            <div class="probability">
              <div>Probability (Math.)</div>
              <div>{{ block.settings.block_text }}</div>
            </div>
            <div class="text">{{ block.settings.block_text_02 }}</div>
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</div>
