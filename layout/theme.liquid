<!doctype html>
{%- capture dir -%}
	{%- case request.locale.iso_code -%}
	  {%- when 'ae' or 'ar' or 'arc' or 'bcc' or 'bqi' or 'ckb' or 'dv' or 'fa' or 'glk' or 'ha' or 'he' or 'kwh' or 'ks' or 'ku' or 'mzn' or 'nqo' or 'pnb' or 'ps' or 'sd' or 'ug' or 'ur' or 'yi' -%}
	    rtl
	  {%- else -%}
	    ltr
	{%- endcase -%}
{%- endcapture -%}
<html class="no-js" lang="{{ request.locale.iso_code }}" dir="{{ dir }}">
  <head>
    <link rel="preload" href="{{ 'OUTFIT-LIGHT.TTF' | asset_url }}" as="font" crossorigin="anonymous">
    <link rel="preload" href="{{ 'OUTFIT-BOLD.TTF' | asset_url }}" as="font" crossorigin="anonymous">
    <link rel="preload" href="{{ 'OUTFIT-REGULAR.TTF' | asset_url }}" as="font" crossorigin="anonymous">
    <link rel="preload" href="{{ 'outfit-semibold.ttf' | asset_url }}" as="font" crossorigin="anonymous">
    <link rel="preload" href="{{ 'BIGMACCA.TTF' | asset_url }}" as="font" crossorigin="anonymous">
    {{- 'adaptation.js' | asset_url | script_tag -}}
    {% comment %}
      <link  rel="preload" href="{{ 'fontawesome-webfont.woff2' | asset_url }}" as="font" crossorigin="anonymous">
      <link  rel="preload" href="{{ 'fontawesome-webfont.woff' | asset_url }}" as="font" crossorigin="anonymous">
      <link  rel="preload" href="{{ 'fontawesome-webfont.ttf' | asset_url }}" as="font" crossorigin="anonymous">
      <link  rel="preload" href="{{ 'fontawesome-webfont.eot' | asset_url }}" as="font" crossorigin="anonymous">
    {% endcomment %}

    <style>
      @font-face {
        font-family: Outfit-Bold;
        src: url('{{ "OUTFIT-BOLD.TTF" | asset_url }}');
      }
      @font-face {
        font-family: Outfit-Light;
        src: url('{{ "OUTFIT-LIGHT.TTF" | asset_url }}');
      }
      @font-face {
        font-family: Outfit-Regular;
        src: url('{{ "OUTFIT-REGULAR.TTF" | asset_url }}');
      }
      @font-face {
        font-family: OUTFIT-SemiBold;
        src: url('{{ "outfit-semibold.ttf" | asset_url }}');
      }
      @font-face {
        font-family: BIGMACCA;
        src: url('{{ "BIGMACCA.TTF" | asset_url }}');
      }
    </style>
