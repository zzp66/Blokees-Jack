<script>
	$(".mobile-menu-drawer--inner .menu-list .menu-item").first().addClass("active");
	$(document).ready(function() {
		$(".mobile-menu-drawer--inner .menu-list .menu-item").click(function(){
			if ($(this).hasClass("active")) {
				// 类存在的处理逻辑
				$(this).removeClass("active");
			} else {
				// 类不存在的处理逻辑
				$(this).addClass("active");
			}
			
		});
	});
	</script>

