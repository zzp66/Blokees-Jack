<nav id="mobile-menu" class="mobile-menu-drawer" role="dialog" tabindex="-1">
	<div class="mobile-menu-drawer--inner">
		{%- if header_settings.menu -%}
			<ul class="mobile-menu">
					{%- for link in header_settings.menu.links -%}
						{%- liquid
							assign parent_index = forloop.index
							assign has_sub_menu = false
							assign has_mega_menu = false
							assign has_menu_badge = false
							assign mega_menu_block_index = false
							assign mega_menu_blocks = blocks | where: "type", "megamenu"
							assign menu_badge_index = false
							assign menu_badge_blocks = blocks | where: "type", "menu_badge"

							if link.links.size > 0
								assign has_sub_menu = true
							endif
							for block in mega_menu_blocks
								assign position = block.settings.position | plus: 0
							if position == parent_index
									assign has_mega_menu = true
									assign mega_menu_block_index = forloop.index0
								endif
							endfor
							for block in menu_badge_blocks
								assign badge_position = block.settings.badge_position | plus: 0
							if badge_position == parent_index
									assign has_menu_badge = true
		
									assign menu_badge_index = forloop.index0
								endif
							endfor
						-%}
						
					  <li>
						{%- if has_sub_menu == true or has_mega_menu == true -%}
								{%- liquid
									assign mega_menu_block = mega_menu_blocks[mega_menu_block_index]
								-%}
								
						<a href="{{ link.url }}" title="{{ link.title | escape }}" style='border-bottom: none;'>
								<details class="link-container"   style='width: -webkit-fill-available; pointer-events: none;'>
									
									<summary class="parent-link">{{ link.title }}{%- if has_menu_badge -%}
										{%- assign menu_badge_block = menu_badge_blocks[menu_badge_index] -%}
										<span class="menu-item-badge" 
											 style="--menu-badge-bg: {{ menu_badge_block.settings.color_bg }};--menu-badge-text: {{ menu_badge_block.settings.color_text }};">
											 {{ menu_badge_block.settings.text }}</span>
										
									{%- endif -%}
									<span class="link-forward" id="link-forward">
									{% comment %} {%- render 'svg-icons' with 'thb-mobile-menu-forward' -%} {% endcomment %}
									<div class='forward-text'>Learn More</div>
									</span>
								</summary>
								
								
									{% comment %} <ul class="sub-menu">
										<li class="parent-link-back">
											<button class="parent-link-back--button"><span>{%- render 'svg-icons' with 'thb-mobile-menu-back' -%}</span> {{ link.title }}</button>
										</li>
										{%- if link.links.size > 0 -%}
									{%- for l in link.links -%}
												{%- liquid
													 assign has_sub_menu = false
												  if l.links.size > 0
													assign has_sub_menu = true
													endif
												-%}
												<li>
													{%- if has_sub_menu -%}
													<details class="link-container">
														<summary>{{ l.title }}<span class="link-forward">{%- render 'svg-icons' with 'thb-mobile-menu-forward' -%}</span></summary>
														<ul class="sub-menu" tabindex="-1">
															<li class="parent-link-back">
																<button class="parent-link-back--button"><span>{%- render 'svg-icons' with 'thb-mobile-menu-back' -%}</span> {{ l.title }}</button>
															</li>
															{%- for sub_link in l.links -%}
																{%- liquid
																	assign has_sub_menu = false
																	if sub_link.links.size > 0
																		assign has_sub_menu = true
																	endif
																-%}
																<li>
																	<a href="{{ sub_link.url }}" title="{{ sub_link.title | escape }}" role="menuitem">{{ sub_link.title }} {% if has_sub_menu %}<span></span>{%- endif -%}</a>
																</li>
															{%- endfor -%}
														</ul>
													</details>
													{%- else -%}
													<a href="{{ l.url }}" title="{{ l.title | escape }}">{{ l.title }}</a>
												  {%- endif -%}
												</li>
											{%- endfor -%}
										{%- endif -%}
										{% if has_mega_menu == true %}
											{%- if mega_menu_block.settings.promo_1_image -%}
												{% render 'header-full-menu-promotion', promotion_image: mega_menu_block.settings.promo_1_image, promotion_link: mega_menu_block.settings.promo_1_link, promotion_heading: mega_menu_block.settings.promo_1_heading, promotion_content: mega_menu_block.settings.promo_1_content %}
											{%- endif -%}
											{%- if mega_menu_block.settings.promo_2_image -%}
												{% render 'header-full-menu-promotion', promotion_image: mega_menu_block.settings.promo_2_image, promotion_link: mega_menu_block.settings.promo_2_link, promotion_heading: mega_menu_block.settings.promo_2_heading, promotion_content: mega_menu_block.settings.promo_2_content %}
											{%- endif -%}
											{%- if mega_menu_block.settings.promo_3_image -%}
												{% render 'header-full-menu-promotion', promotion_image: mega_menu_block.settings.promo_3_image, promotion_link: mega_menu_block.settings.promo_3_link, promotion_heading: mega_menu_block.settings.promo_3_heading, promotion_content: mega_menu_block.settings.promo_3_content %}
											{%- endif -%}
											{%- if mega_menu_block.settings.promo_4_image -%}
												{% render 'header-full-menu-promotion', promotion_image: mega_menu_block.settings.promo_4_image, promotion_link: mega_menu_block.settings.promo_4_link, promotion_heading: mega_menu_block.settings.promo_4_heading, promotion_content: mega_menu_block.settings.promo_4_content %}
											{%- endif -%}
										{%- endif -%}
								</ul> {% endcomment %}
								</details>
							</a>
							{%- else -%}
							<a href="{{ link.url }}" title="{{ link.title | escape }}">{{ link.title }}{%- if has_menu_badge -%}
								{%- assign menu_badge_block = menu_badge_blocks[menu_badge_index] -%}
								<span class="menu-item-badge" style="--menu-badge-bg: {{ menu_badge_block.settings.color_bg }};--menu-badge-text: {{ menu_badge_block.settings.color_text }};">{{ menu_badge_block.settings.text }}</span>
							{%- endif -%}</a>
						{%- endif -%}
					  </li>
					  {% if has_sub_menu  %}
						{% comment %} 写入 {% endcomment %}

						<div class="menu-list">
							{% for block in blocks %}
							{% if block.type == 'menulist' and block.settings.menu_name == link.title %}
								<div class="menu-item" >
									<div class='line-left'>
										<div class='left-tag'>
										{% for item in block.settings.collection.all_tags %}
											<div class='tag-item'> 
											{{ item }}
											</div>
										{% endfor %}
										
										</div>
										<div class='left-text'>
										<div class='left-title'>
											{{ block.settings.collection.title }}
										</div>
										<div class='left-subtitle'>
											<svg xmlns="http://www.w3.org/2000/svg" width="27" height="27" viewBox="0 0 27 27" fill="none">
												<path d="M27 13.5C27 6.04416 20.9558 3.51916e-07 13.5 2.26709e-07C6.04416 1.01501e-07 2.3843e-06 6.04416 1.53599e-06 13.5C6.87687e-07 20.9558 6.04416 27 13.5 27C20.9558 27 27 20.9558 27 13.5Z" fill="#FF5F00"/>
												<mask id="mask0_405_693" style="mask-type:luminance" maskUnits="userSpaceOnUse" x="8" y="7" width="11" height="13">
												<path d="M8.31446 7.91295L8.31445 19.543L18.6196 19.543L18.6196 7.91295L8.31446 7.91295Z" fill="white"/>
												</mask>
												<g mask="url(#mask0_405_693)">
												<path d="M13.4673 7.91295L14.3015 7.91295L14.3015 16.0679C14.3015 14.055 18.6196 13.3325 18.6196 13.3325L18.6196 15.5344C15.489 15.6894 14.3881 19.543 14.3881 19.543L12.5469 19.543C12.5469 19.543 11.446 15.6894 8.31445 15.5344L8.31445 13.3325C8.31445 13.3325 12.6331 14.055 12.6331 16.0679L12.6331 7.91295L13.4673 7.91295Z" fill="white"/>
												</g>
											</svg>
											
										</div>
										</div> 
										<div class='left-content'> 
										{{ block.settings.collection.description }}
										</div>
									</div>
									
									{% assign prosize = block.settings.collection.products | size %}
									{% if prosize >3 %}
									<div class='line-right line-right-frist'> 
									{% for item in block.settings.collection.products %}
										<a href="{{ item.url }}" class='right-item ' >
										{% assign content = item.metafields.custom.title_grop.value.meshi.value %}
										{% assign img = item.metafields.custom.title_grop.value.myimg.value %}
										<img src="{{ img | image_url }}"> 
										<div class='item-title'>{{  content }}</div>
										</a>
									{% endfor %}
										
									</div>
									{% endif %}
									{% if prosize <= 3 %}
									<div class='line-right line-right-last'> 
									{% for item in block.settings.collection.products %}
										<a href="{{ item.url }}"  class='right-item'>
										{% assign content = item.metafields.custom.title_grop.value.meshi.value %}
										{% assign img = item.metafields.custom.title_grop.value.myimg.value %}
										<img src="{{ img | image_url }}"> 
										<div class='item-title'>{{  content }}</div>
										</a>
									{% endfor %}
										
									</div>
									
									{% endif %}
								
								</div>
							{% endif %}
							{% endfor %} 
						</div> 
						{% comment %} 结束写入 {% endcomment %}
					{% endif %}
				  {%- endfor -%}
			</ul>
				{%- endif -%}
				{%- if header_settings.mobile_secondary_menu != blank -%}
				<ul class="mobile-secondary-menu">
					{%- for link in header_settings.mobile_secondary_menu.links -%}
					  <li class="{% if link.active %} active{% endif %}">
						<a href="{{ link.url }}" title="{{ link.title | escape }}">{{ link.title }}</a>
					  </li>
				  {%- endfor -%}
			</ul>
				{%- endif -%}

				{%- if header_settings.show_social_links -%}
					{%- render 'social-links', show_label: false -%}
				{%- endif -%}
				<div class="thb-mobile-menu-footer">
					{%- if shop.customer_accounts_enabled -%}
						{%- if customer -%}
						  {%- assign profile_link = routes.account_url -%}
						  {%- capture profile_text -%}
							{{ 'sections.header.my_account' | t }}
						  {%- endcapture -%}
						{%- else -%}
						  {%- assign profile_link = routes.account_login_url -%}
						  {%- capture profile_text -%}
							{{ 'sections.header.login' | t }}
						  {%- endcapture -%}
						{%- endif -%}
					<a class="thb-mobile-account-link" href="{{ profile_link | escape }}" title="{{ profile_text | escape }}">{%- render 'svg-icons' with 'myaccount' %} {{ profile_text }}</a>
				  {%- endif -%}
					<div class="no-js-hidden">
						{%- render 'localization', id: 'MobileMenuLocalization', show_language_switcher: header_settings.show_language_switcher, show_currency_switcher: header_settings.show_currency_switcher, open_top: true -%}
					</div>
				</div> 


				{% comment %}  {% endcomment %}
		 
	</div>
</nav>
